    "def chanker(marked_text):\n",
    "    neg_verb = []\n",
    "    adj_noun = []\n",
    "    adv_verb = []\n",
    "    for i in range(len(marked_text)-1):\n",
    "        if marked_text[i][0] == 'не' and marked_text[i+1][1] == 'VERB':\n",
    "            neg_verb.append([marked_text[i][0], marked_text[i+1][0]])\n",
    "        if marked_text[i][1] == 'ADJ' and marked_text[i+1][1] == 'NOUN':\n",
    "            adj_noun.append([marked_text[i][0], marked_text[i+1][0]])\n",
    "        if marked_text[i][1] == 'ADV' and marked_text[i+1][1] == 'VERB':\n",
    "            adv_verb.append([marked_text[i][0], marked_text[i+1][0]])\n",
    "    return neg_verb, adj_noun, adv_verb"
